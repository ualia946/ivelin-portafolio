import Footer from "../components/commons/footer/footer.astro";
import { ChatBot } from "../components/commons/chatBot/ChatBot.jsx"
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <meta name="apple-mobile-web-app-title" content="Ivelin Portafolio" />
    <link rel="manifest" href="/site.webmanifest" />
    <meta name="generator" content={Astro.generator} />
    <title>Ivelin Iliyanov Apostolov</title>
  </head>
  <body class="bg-gray-950 text-white">
    <Navbar />
    <slot />
    <ChatBot client:load/>
    <Footer />
  </body>
</html>
